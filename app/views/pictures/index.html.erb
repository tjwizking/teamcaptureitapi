<h1>Listing pictures</h1>

<table>
  <thead>
    <tr>
      <th>Picture</th>
      <th>Geox</th>
      <th>Geoy</th>
      <th>Uploadtime</th>
      <th>Category</th>
      <th>Uploadedby</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pictures.each do |picture| %>
      <tr>
        <td><%= picture.picture %></td>
        <td><%= picture.geox %></td>
        <td><%= picture.geoy %></td>
        <td><%= picture.uploadTime %></td>
        <td><%= picture.category %></td>
        <td><%= picture.uploadedBy %></td>
        <td><%= link_to 'Show', picture %></td>
        <td><%= link_to 'Edit', edit_picture_path(picture) %></td>
        <td><%= link_to 'Destroy', picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Picture', new_picture_path %>
